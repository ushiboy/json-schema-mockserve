{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "rules": {
        "quotes": 0,
        "no-underscore-dangle": 0,
        "no-mixed-requires": 0,
        "no-unused-vars": 0,
        "no-process-exit": 0,
        "no-loop-func": 0,
        "camelcase": 0,
        "no-shadow": 0
    },
    "globals": {
    }
}
